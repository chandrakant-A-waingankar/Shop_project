        {
            "name": "RobotCode: Default",
            "type": "robotcode",
            "request": "launch",
            "purpose": "default",
            "presentation": {
                "hidden": true
            },
            "pythonConfiguration": "RobotCode: Python"
        },
        {
            "name": "RobotCode: Python",
            "type": "debugpy",
            "request": "attach",
